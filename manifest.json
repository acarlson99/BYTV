{
	"name": "bytv",
	"manifest_version": 2,
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"*://www.youtube.com/*",
				"*://youtube.com/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"https://api.betterttv.net/*",
		"*://www.youtube.com/*",
		"*://youtube.com/*"
	]
}
